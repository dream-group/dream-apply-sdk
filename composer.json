    "require": {
        "php": ">= 5.5",
        "danielstjules/stringy": "^2.0 || ^3.0",
        "fig/http-message-util": "^1.1",
        "guzzlehttp/guzzle": "^6.2 || ^7.0"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "*",
        "phpcompatibility/php-compatibility": "*",
        "psy/psysh": "*",
        "squizlabs/php_codesniffer": "*"
    }
}
