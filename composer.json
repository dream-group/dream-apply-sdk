{
    "name": "dream-group/dream-apply-sdk",
    "description": "Dream Apply SDK",
    "type": "library",
    "license": "private",
    "authors": [
        {
            "name": "Anton Smirnov",
            "email": "anton.smirnov@dreamapply.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Dream\\Apply\\Client\\": "src"
        }
